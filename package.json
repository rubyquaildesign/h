{
  "name": "@rupertofly/h",
  "sideEffects": false,
  "version": "4.3.1",
  "type": "module",
  "description": "A set of useful functions and types for me",
  "main": "./dist/main.cjs",
  "module": "./dist/main.js",
  "exports": {
    ".": {
      "import": "./dist/main.js",
      "require": "./dist/main.cjs"
    }
  },
  "repository": "git@github.com:Rupertofly/h.git",
  "author": "Ruby Quail <ruby@rubyquail.design>",
  "license": "MIT",
  "private": false,
  "xo": {
    "space": 2,
    "prettier": true,
    "rules": {
      "unicorn/filename-case": 0,
      "unicorn/no-array-reduce": 0,
      "unicorn/prefer-node-protocol": 0,
      "import/no-unassigned-import": 0
    }
  },
  "prettier": {
    "useTabs": false,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "all",
    "bracketSpacing": true
  },
  "files": [
    "dist"
  ],
  "types": "dist/main.d.ts",
  "scripts": {
    "build:types": "tsc -m amd -t ESNext -d --isolatedModules false  --moduleResolution node --esModuleInterop --jsx react --outFile dist/index.js src/main.ts && rm -rf dist/index.js ",
    "build": "tsup",
    "watch": "tsup -w",
    "dev": "vite",
    "test": "node --experimental-specifier-resolution=node --loader ts-node/esm ./test/tests.ts",
    "svg-test": "node --experimental-specifier-resolution=node --loader ts-node/esm ./test/svgTest.ts",
    "test-nyan": "node --experimental-specifier-resolution=node --loader ts-node/esm ./test/tests.ts | tap-nyan",
    "lint": "xo ./src/*",
    "prepare": "yarn run build",
    "prepublishOnly": "yarn test && yarn run lint",
    "preversion": "yarn run lint"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^11.1.0",
    "@rollup/plugin-node-resolve": "^7.1.3",
    "@rollup/plugin-typescript": "^8.3.0",
    "@types/b-spline": "^2.0.0",
    "@types/d3-array": "^2.0.0",
    "@types/d3-delaunay": "^5.3.0",
    "@types/d3-path": "^1.0.8",
    "@types/d3-polygon": "^2.0.0",
    "@types/jsdom": "^16.2.1",
    "@types/node": "^13.11.1",
    "@types/rollup": "^0.54.0",
    "@types/tape": "^4.2.34",
    "d3-delaunay": "^6.0.2",
    "eslint-config-xo-typescript": "^0.44.0",
    "jsdom": "^16.2.2",
    "prettier": "^2.0.4",
    "rollup": "^2.6.1",
    "snowpack": "^3.8.6",
    "tap-nyan": "^1.1.0",
    "tape": "^4.13.2",
    "ts-node": "^10.0.0",
    "tsup": "^5.4.0",
    "typescript": "^3.8.3",
    "vite": "^2.6.7",
    "vite-dts": "^1.0.3",
    "xo": "0.42"
  },
  "dependencies": {
    "b-spline": "^2.0.1",
    "d3-array": "^2.5.1",
    "d3-path": "^1.0.9",
    "d3-polygon": "^2.0.0",
    "tinyqueue": "^2.0.3",
    "tslib": "^1.11.1"
  }
}
