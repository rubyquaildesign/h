{
  "name": "@rupertofly/h",
  "sideEffects": false,
  "version": "4.1.0",
  "description": "A set of useful functions and types for me",
  "main": "dist/index.js",
  "repository": "git@github.com:Rupertofly/h.git",
  "author": "Ruby Quail <ruby@rubyquail.design>",
  "license": "MIT",
  "private": false,
  "xo": {
    "space": 2,
    "prettier": true,
    "rules": {
      "unicorn/filename-case": 0,
      "unicorn/no-array-reduce": 0,
      "@typescript-eslint/no-unused-vars": 1
    }
  },
  "prettier": {
    "useTabs": false,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "all"
  },
  "files": [
    "dist",
    "types"
  ],
  "types": "types/index.d.ts",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "xo && node --experimental-specifier-resolution=node --loader ts-node/esm ./test/tests.ts",
    "test-nyan": "node --experimental-specifier-resolution=node --loader ts-node/esm ./test/tests.ts | tap-nyan",
    "prepublish": "yarn build",
    "prepublishOnly": "yarn test"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^11.1.0",
    "@rollup/plugin-node-resolve": "^7.1.3",
    "@rollup/plugin-typescript": "^4.1.1",
    "@types/b-spline": "^2.0.0",
    "@types/d3-array": "^2.0.0",
    "@types/d3-delaunay": "^5.3.0",
    "@types/d3-path": "^1.0.8",
    "@types/d3-polygon": "^2.0.0",
    "@types/jsdom": "^16.2.1",
    "@types/node": "^13.11.1",
    "@types/rollup": "^0.54.0",
    "@types/tape": "^4.2.34",
    "d3-delaunay": "^6.0.2",
    "eslint-config-xo-typescript": "^0.44.0",
    "jsdom": "^16.2.2",
    "prettier": "^2.0.4",
    "rollup": "^2.6.1",
    "snowpack": "^3.8.6",
    "tap-nyan": "^1.1.0",
    "tape": "^4.13.2",
    "ts-node": "^10.0.0",
    "typescript": "^3.8.3",
    "xo": "0.42"
  },
  "dependencies": {
    "b-spline": "^2.0.1",
    "d3-array": "^2.5.1",
    "d3-path": "^1.0.9",
    "d3-polygon": "^2.0.0",
    "tinyqueue": "^2.0.3",
    "tslib": "^1.11.1"
  }
}
